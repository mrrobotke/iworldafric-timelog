{
  "name": "@iworldafric/timelog",
  "version": "0.1.4",
  "description": "Advanced Time Log system for iWorldAfric developer platform",
  "author": "Antony Ngigge <antony@iworldafric.com>",
  "license": "MIT",
  "homepage": "https://github.com/Mrrobotke/iworldafric-timelog#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mrrobotke/iworldafric-timelog.git"
  },
  "bugs": {
    "url": "https://github.com/Mrrobotke/iworldafric-timelog/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./adapters/prisma": {
      "types": "./dist/adapters/prisma/index.d.ts",
      "import": "./dist/adapters/prisma/index.js",
      "require": "./dist/adapters/prisma/index.cjs"
    },
    "./server/next": {
      "types": "./dist/server/next/index.d.ts",
      "import": "./dist/server/next/index.js",
      "require": "./dist/server/next/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    }
  },
  "files": [
    "dist",
    "scripts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist coverage .turbo",
    "postinstall": "node scripts/postinstall.cjs",
    "prepublishOnly": "npm run build && npm run test"
  },
  "dependencies": {
    "date-fns": "^4.0.0",
    "zod": "^4.0.0",
    "@hookform/resolvers": "^5.2.0",
    "react-hook-form": "^7.61.1"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.0.0",
    "@prisma/client": "^6.14.0",
    "@tanstack/react-query": "^5.0.0",
    "next": "^15.4.4",
    "next-auth": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.0.0",
    "recharts": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@chakra-ui/react": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    },
    "recharts": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/ui": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "jsdom": "^25.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "timelog",
    "timesheet",
    "time-tracking",
    "nextjs",
    "react",
    "prisma",
    "typescript"
  ]
}
